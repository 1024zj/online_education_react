<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ff.dao.QuestionMapper">
	<resultMap id="videoMap" type="com.ff.pojo.Question">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="chapter_id" property="chapterId" />
		<result column="v_order" property="order" />
		<result column="url" property="url" />
	</resultMap>



	<!-- 增加新视频 -->
	<insert id="insert" parameterType="com.ff.pojo.Question"
		keyProperty="id" useGeneratedKeys="true">
		insert into questions
		(title,video_id,textA,textB,textC,textD,answer)
		values (#{title},
		#{chapterId},#{textA},#{textB},#{textC},#{textD},#{answer})
	</insert>

</mapper>